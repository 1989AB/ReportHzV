                  description="Der auszuführende SQL-Befehl."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- BSNR Join vorerst ausgesetzt da die Adressen nicht BSNRspezifisch vorhanden sind.
select 
	t1.VERTRAGS_ID,
	case when SUBSTRING(t1.[VERTRAGS_ID],4,2)='99' then '52' else SUBSTRING(t1.[VERTRAGS_ID],4,2) end as VertragsKV,
	t1.L_ARZT_NR as LANR7,
	t1.L_ARZT_BS_FG as LANR_FG,
	null as BSNR9,				--concat(t2.BS_NR,t2.BSN_KV_ERW),
	null as BSNR_GueltigVon,	--t2.GUELTIG_VON,
	null as BSNR_GueltigBis,	--t2.GUELTIG_BIS,
	t3.ANREDE,
	t3.Vorname,
	t3.Nachname,
	t3.Strasse,
	t3.PLZ,
	t3.Ort,
	t1.GUELTIG_VON as TeilnahmeBeginn,
	t1.GUELTIG_BIS as TeilnahmeEnde,
	t1.ENDEGRUND,
	t1.endegrund_text,
	null as AnzahlVersicherte
from [V_HZV].[V_HZV_Arzt_Teilnahme] t1
--inner join [V_HZV].[V_HZV_Arzt_BS] t2
--on t1.L_ARZT_NR		=	t2.L_ARZT_NR
--and t1.L_ARZT_BS_FG	=	t2.L_ARZT_BS_FG
--and t1.VERTRAGS_ID	=	t2.VERTRAGS_ID
inner join [V_HZV].[V_HZV_Arzt] t3
on t1.L_ARZT_NR		=	t3.LANR7
and t1.L_ARZT_BS_FG	=	t3.FACHGRUPPE
and t1.VERTRAGS_ID	=	t3.VERTRAGS_ID
where t1.GUELTIG_BIS&gt;cast (concat(year(getdate())-5,'-12-31') as date)</property>
